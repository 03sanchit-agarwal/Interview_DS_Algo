typedef long long ll;
class MinStack {
public:

    stack<pair<ll, ll>> st;
    MinStack() {
        
    }
    
    void push(int val) {
        if(st.empty()) {
            st.push({val, val});
        } else {
            ll temp = min((ll)val, st.top().second);
            st.push({val, temp});
        }
    }
    
    void pop() {
        st.pop();
    }
    
    int top() {
        return st.top().first;
    }
    
    int getMin() {
        return st.top().second;
    }
};
